{
  "name": "syncydink",
  "version": "0.0.2",
  "description": "2D and VR Movie Player with Haptics Integration and Synchronization",
  "author": "Kyle Machulis <kyle@machul.is>",
  "private": true,
  "scripts": {
    "trash": "trash dist dist-bundle",
    "start": "yarn build:config && webpack-dev-server --open --hot --config build/webpack.base.js",
    "start:secure": "yarn build:config && webpack-dev-server --open --hot --https  --config build/webpack.base.js",
    "build:config": "node build/generate_config.js",
    "build:dev": "yarn trash && yarn build:config && webpack --progress --hide-modules --config build/webpack.base.js",
    "build": "yarn trash && yarn build:config && webpack --progress --hide-modules  --config build/webpack.production.js",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "build:main": "yarn trash && yarn build:config && tsc -p tsconfig.json",
    "pretest": "yarn build:main",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --compilers ts:ts-node/register 'tests/**/*.ts'",
    "deploy": "trash deploy && mkdir -p deploy && cp index.html deploy && cp -r dist deploy"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "aframe": "^0.8.2",
    "aframe-stereo-component": "^0.6.0",
    "buttplug": "^0.7.1",
    "d3": "^5.1.0",
    "haptic-movie-file-reader": "^0.0.10",
    "mousetrap": "^1.6.1",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.5.16",
    "vue-awesome": "^3.0.0",
    "vue-buttplug-material-component": "^0.2.1",
    "vue-touch": "next",
    "vue-video-player": "^5.0.2",
    "vuetify": "^1.0.17"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^9.4.6",
    "@types/tween.js": "^16.9.0",
    "@types/video.js": "^6.2.4",
    "chai": "^4.1.2",
    "codecov": "^3.0.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "less": "^3.0.2",
    "less-loader": "^4.1.0",
    "mocha": "^5.1.1",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.7.1",
    "style-loader": "^0.21.0",
    "trash": "^4.3.0",
    "trash-cli": "^1.4.0",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.7",
    "vue-property-decorator": "^6.0.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "pre-push": []
}
