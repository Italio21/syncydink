{
  "name": "syncydink",
  "version": "0.0.2",
  "description": "2D and VR Movie Player with Haptics Integration and Synchronization",
  "author": "Kyle Machulis <kyle@machul.is>",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "start:secure": "cross-env NODE_ENV=development webpack-dev-server --open --hot --https",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "webpack-defaults": "webpack-defaults",
    "build:main": "tsc -p tsconfig.json",
    "pretest": "yarn build:main",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --compilers ts:ts-node/register 'tests/**/*.ts'"
  },
  "dependencies": {
    "aframe": "^0.7.0",
    "aframe-stereo-component": "^0.6.0",
    "buttplug": "^0.2.1",
    "d3": "^4.11.0",
    "haptic-movie-file-reader": "^0.0.7",
    "mousetrap": "^1.6.1",
    "tslint": "^5.7.0",
    "video.js": "^6.2.8",
    "viewport-units-buggyfill": "^0.6.2",
    "vue": "^2.5.2",
    "vue-awesome": "^2.3.3",
    "vue-buttplug-material-component": "^0.0.2",
    "vue-material": "^0.7.5",
    "vue-touch": "next",
    "vue-video-player": "^3.1.4"
  },
  "devDependencies": {
    "@types/aframe": "^0.5.0",
    "@types/chai": "^4.0.4",
    "@types/d3": "^4.11.0",
    "@types/mocha": "^2.2.43",
    "@types/mousetrap": "^1.5.34",
    "@types/node": "^8.0.34",
    "@types/video.js": "^6.2.0",
    "chai": "^4.1.2",
    "codecov": "^2.3.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "mocha": "^4.0.1",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "nyc": "^11.2.1",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3",
    "uglify-es": "^3.1.3",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.3.0",
    "vue-property-decorator": "^6.0.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1"
  },
  "pre-push": []
}
